function L02E(A, B, C) {
  if (A == 0) {
    return "A não pode ser zero.";
  }
  var delta = B * B - 4 * A * C;
  if (delta < 0) {
    return "Raízes não reais.";
  }
  var raizDelta = Math.sqrt(delta);
  var x1 = (-B + raizDelta) / (2 * A);
  var x2 = (-B - raizDelta) / (2 * A);
  return "Raízes: x1 = " + x1 + ", x2 = " + x2;
}
